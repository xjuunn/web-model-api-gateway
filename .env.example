# 日志级别: debug / info / warn / error
LOG_LEVEL=info

# API 服务监听主机
APP_HOST=localhost
# API 服务监听端口
APP_PORT=9091
# 默认启动模式: auto / webai / native-api
APP_DEFAULT_MODE=auto
# 当前活动网页模型 Provider 标识
APP_ACTIVE_PROVIDER=gemini-web

# 是否启用 Gemini 网页模型能力
ENABLE_GEMINI=true
# 读取浏览器 Cookie 的来源类型
GEMINI_BROWSER=chrome
# Gemini 默认模型名
GEMINI_DEFAULT_MODEL=gemini-2.5-flash
# Gemini Cookie: __Secure-1PSID
GEMINI_COOKIE_1PSID=
# Gemini Cookie: __Secure-1PSIDTS
GEMINI_COOKIE_1PSIDTS=
# Gemini 请求代理地址(可空)
GEMINI_HTTP_PROXY=
# 是否允许自动从浏览器读取 Cookie
GEMINI_ALLOW_BROWSER_COOKIES=false
# 初始化失败时是否保存 HTML 调试文件
GEMINI_DEBUG_SAVE_INIT_HTML=false
# 代理初始化失败时是否直连重试
GEMINI_RETRY_WITHOUT_PROXY=false
