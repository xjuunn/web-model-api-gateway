{
  "name": "web-model-api-gateway",
  "version": "1.0.0",
  "private": true,
  "description": "Type-safe TypeScript gateway for Gemini WebAI with interactive CLI",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "build": "tsc -p tsconfig.json",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ci": "vitest run --coverage --reporter=default",
    "api:test": "tsx scripts/api-endpoint-test.ts",
    "chat:test": "npm run build && node scripts/chat-tester.mjs",
    "onetest": "tsx scripts/onetest.ts"
  },
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "@hono/node-server": "^1.19.9",
    "ai": "^6.0.82",
    "chrome-cookies-secure": "^3.0.1",
    "hono": "^4.11.9",
    "mime-types": "^3.0.1",
    "picocolors": "^1.1.1",
    "prompts": "^2.4.2",
    "undici": "^7.15.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/mime-types": "^3.0.1",
    "@types/node": "^22.13.4",
    "@types/prompts": "^2.4.9",
    "@types/supertest": "^6.0.3",
    "@vitest/coverage-v8": "^3.2.4",
    "supertest": "^7.1.3",
    "tsx": "^4.19.2",
    "typescript": "^5.8.2",
    "vitest": "^3.2.4"
  }
}
